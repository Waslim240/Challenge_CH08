<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="waslim.binar.andlima.challengech08">

    <uses-permission android:name="android.permission.INTERNET"/>

    <application
        android:allowBackup="true"
        android:name=".di.MyApplication"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.ChallengeCH08"
        android:usesCleartextTraffic="true"
        tools:targetApi="m">
        <activity
            android:name=".view.DetailLayout"
            android:exported="false"
            android:label="@string/title_activity_detail_layout"
            android:theme="@style/Theme.ChallengeCH08" />
        <activity
            android:name=".view.HomeLayout"
            android:exported="false"
            android:label="@string/title_activity_home_layout"
            android:theme="@style/Theme.ChallengeCH08" />
        <activity
            android:name=".view.RegisterLayout"
            android:exported="false"
            android:label="@string/title_activity_register_layout"
            android:theme="@style/Theme.ChallengeCH08" />
        <activity
            android:name=".view.MainActivity"
            android:exported="true"
            android:theme="@style/Theme.ChallengeCH08">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>